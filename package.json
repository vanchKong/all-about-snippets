{
  "name": "snippets",
  "displayName": "all-about-snippets",
  "description": "vue2/3 snippets",
  "icon": "public/logo.png",
  "version": "0.0.1",
  "license": "MIT",
  "author": {
    "name": "vanch",
    "email": "vanchkong@qq.com",
    "url": "https://github.com/VanchKong"
  },
  "engines": {
    "vscode": "^1.75.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "vue",
    "vue2",
    "vue3",
    "vuejs",
    "vue.js",
    "composition-api",
    "composition",
    "html",
    "javascript"
  ],
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".vue"
        ],
        "configuration": "./languages/vue-language-configuration.json"
      },
      {
        "id": "vue-html",
        "aliases": [
          "Vue-html",
          "vue-html"
        ],
        "configuration": "./languages/vue-html-language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "vue",
        "scopeName": "source.vue",
        "path": "./syntaxes/vue-generated.json",
        "embeddedLanguages": {
          "text.html.basic": "html",
          "text.html.vue-html": "vue-html",
          "text.jade.slm": "slm",
          "text.pug": "jade",
          "source.css": "css",
          "source.css.scss": "scss",
          "source.css.less": "less",
          "source.js": "javascript",
          "source.ts": "typescript"
        }
      },
      {
        "language": "vue-html",
        "scopeName": "text.html.vue-html",
        "path": "./syntaxes/vue-html.tmLanguage.json",
        "embeddedLanguages": {
          "source.js": "javascript"
        }
      },
      {
        "path": "./syntaxes/pug/directives.tmLanguage.json",
        "scopeName": "vue.pug.directives",
        "injectTo": [
          "source.vue"
        ]
      },
      {
        "path": "./syntaxes/pug/interpolations.tmLanguage.json",
        "scopeName": "vue.pug.interpolations",
        "injectTo": [
          "source.vue"
        ]
      }
    ],
    "snippets": [
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue-html.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/vue-js.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/vue-js.json"
      },
      {
        "language": "javascript",
        "path": "./snippets/basic/javascript.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/basic/javascript.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/basic/html.json"
      },
      {
        "language": "jade",
        "path": "./snippets/pug.json"
      },
      {
        "language": "css",
        "path": "./snippets/basic/css.json"
      },
      {
        "language": "less",
        "path": "./snippets/basic/css.json"
      },
      {
        "language": "scss",
        "path": "./snippets/basic/css.json"
      }
    ]
  }
}
