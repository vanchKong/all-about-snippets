		"prefix": "$(record-small)med",
		"body": "@media screen and (${1:max-width: 300px}) {\n  $0\n}"
	},
	"!important": {
		"prefix": "$(record-small)!",
		"body": "!important"
	},
	"!important (alt)": {
		"prefix": "$(record-small)i",
		"body": "!important"
	}
}