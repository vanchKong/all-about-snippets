  "@import": {
    "prefix": "$(record-small)@import",
    "body": "@import '${0:filename}';"
  },
  "@mixin": {
    "prefix": "$(record-small)@mixin",
    "body": "@mixin ${1:name} {\n  $0\n}"
  },
  "@include": {
    "prefix": "$(record-small)@include",
    "body": "@include ${0:mixin};"
  },
  "@keyframes": {
    "prefix": "$(record-small)@keyframes",
    "body": "@keyframes ${1:name} {\n  $0\n}"
  },
  "@media": {
    "prefix": "$(record-small)@media",
    "body": "@media screen and (${1:max-width: 300px}) {\n  $0\n}"
  },
  "!important": {
    "prefix": "$(record-small)!important",
    "body": "!important"
  },
  "absolute": {
    "prefix": "$(record-small)absolute",
    "body": "!important"
  }
  // "!important (alt)": {
  //   "prefix": "$(record-small)i",
  //   "body": "!important"
  // }
}
